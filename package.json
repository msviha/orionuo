{
  "name": "orionuo",
  "version": "1.8.0",
  "description": "Scripts for OrionUO assistant on Darkparadise.cz",
  "scripts": {
    "lint": "eslint ./**/*.ts --quiet --fix",
    "build": "tsc",
    "build-all": "tsc && npm run build-config && npm run build-refill && npm run build-ultimate-rr",
    "build-config": "tsc -p config",
    "build-refill": "tsc -p _dependent/refill-manager",
    "build-ultimate-rr": "tsc -p _dependent/ultimate-rr",
    "watch": "tsc -w",
    "doc": "typedoc src"
  },
  "author": "Michal Sviha",
  "license": "ISC",
  "dependencies": {
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "typedoc": "0.19.2",
    "typedoc-plugin-markdown": "3.0.9"
  }
}
